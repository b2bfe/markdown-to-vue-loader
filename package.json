{
  "name": "markdown-to-vue-loader",
  "version": "3.1.0",
  "description": "Markdown to Vue component loader for Webpack.",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "build:docs": "webpack --mode production",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clear:docs": "del-cli docs",
    "serve": "npm run clean:docs && npm run serve:docs",
    "serve:docs": "webpack serve --mode development",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint {src,test}/**/*.{css,scss,vue,html,md} --fix",
    "lint:js": "eslint src test *.js --ext .js,.vue --fix",
    "prepare": "husky install",
    "release": "npm run lint && npm test && npm run release:docs && npm run changelog",
    "release:docs": "npm run clean:docs && npm run build:docs",
    "start": "npm run serve",
    "test": "nyc mocha --timeout 10000",
    "test:coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fengyuanchen/markdown-to-vue-loader.git"
  },
  "keywords": [
    "markdown",
    "vue",
    "vue2",
    "vue3",
    "vue-component",
    "webpack",
    "webpack5",
    "webpack-loader",
    "front-end",
    "node"
  ],
  "author": {
    "name": "Chen Fengyuan",
    "url": "https://chenfengyuan.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fengyuanchen/markdown-to-vue-loader/issues"
  },
  "homepage": "https://fengyuanchen.github.io/markdown-to-vue-loader",
  "dependencies": {
    "cheerio": "1.0.0-rc.3",
    "markdown-it": "^12.0.4",
    "postcss": "^8.2.2"
  },
  "peerDependencies": {
    "vue-loader": ">=15.0.0",
    "webpack": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@vue/compiler-sfc": "^3.0.5",
    "babel-loader": "^8.2.2",
    "chai": "^4.2.0",
    "codecov": "^3.8.1",
    "conventional-changelog-cli": "^2.1.1",
    "css-loader": "^5.0.1",
    "del-cli": "^3.0.1",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^7.4.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.3",
    "memory-fs": "^0.5.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "postcss-loader": "^4.1.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "vue": "^3.0.4",
    "vue-loader": "^16.1.2",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1",
    "webpack-merge": "^5.7.3"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not ie <= 10"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "{src,test}/**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "{src,test}/**/*.{css,scss,vue,html,md}": [
      "stylelint --fix",
      "git add"
    ]
  }
}
